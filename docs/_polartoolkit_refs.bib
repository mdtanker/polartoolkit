@article{stalantarctic2020b,
  title = {The {{Antarctic Crust}} and {{Upper Mantle}}: {{A Flexible 3D Model}} and {{Software Framework}} for {{Interdisciplinary Research}}},
  shorttitle = {The {{Antarctic Crust}} and {{Upper Mantle}}},
  author = {St{\aa}l, Tobias and Reading, Anya M. and Halpin, Jacqueline A. and Phipps, Steven J. and Whittaker, Joanne M.},
