  booktitle = {Antarctica: {{Contributions}} to {{Global Earth Sciences}}},
  author = {Golynsky, Alexander and Chiappini, Massimo and Damaske, Detlef and Ferraccioli, Fausto and Finn, Carol A. and Ishihara, Takemi and Kim, Hyung Rae and Kovacs, Luis and Masolov, Valery N. and Morris, Peter and {von Frese}, Ralph},
  editor = {F{\"u}tterer, Dieter Karl and Damaske, Detlef and Kleinschmidt, Georg and Miller, Hubert and Tessensohn, Franz},
  year = 2006,
